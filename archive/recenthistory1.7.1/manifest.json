{
  "name": "Recent History",
  "version": "1.7.1",
  "description": "This extension displays your recent history and recently closed tabs in a one click popup menu",
  "options_page": "options.html",
  "background_page": "background.html",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "images/icon16.png",
    "19": "images/icon19.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "page_action": {
    "popup": "popup.html"
  }
}
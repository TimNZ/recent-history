{
  "name": "Recent History",
  "version": "1.3",
  "description": "Displays your recent history in a one click menu like Firefox",
  "options_page": "options.html",
  "background_page": "background.html",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_title": "Recent History",
    "default_icon": "images/icon16.png",
    "popup": "popup.html"
  }
}